<p id="notice"></p>
<h1>Books</h1>
<%= @books.each do |book| %>

<p>Title</p>
 <span>
    <%= book.title %>
 </span>

 <h2>New book</h2>
 <%= form_with model: Book.new do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>

<% end %>